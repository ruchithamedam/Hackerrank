#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    float n;
    scanf("%f",&n);
    printf("%.0f\n",n);
    printf("%.1f\n",ceil(n));
    printf("%.1f",floor(n));
    
    return 0;
}
