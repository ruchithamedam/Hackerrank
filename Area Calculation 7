#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int side,length,breadth,asq,arec;
    float acir,radius;
    scanf("%d %d %d %f",&side,&length,&breadth,&radius);
    asq=side*side;
    arec=length*breadth;
    acir=3.14*radius*radius;
    printf("%d\n%d\n%.2f",asq,arec,acir);
    
    return 0;
}
