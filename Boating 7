#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int b,a,c,d;
    scanf("%d %d %d",&b,&a,&c);
    d=(a*75)+(c*30);
    if(b>=d)
    {
        printf("Boat is stable");    
    }
    else
    {
        printf("Boat will drown");
    }
    return 0;
}
