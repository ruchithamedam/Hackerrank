#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    float x1,y1,x2,y2,x3,y3;
    scanf("%f %f %f %f %f %f",&x1,&y1,&x2,&y2,&x3,&y3);
    float a=(x1+x2+x3)/3;
    float b=(y1+y2+y3)/3;
    printf("%.1f\n%.1f",a,b);
    return 0;
}
